<p id="notice"><%= notice %></p>

<h1>Listing League Roster Slots</h1>

<table>
  <thead>
    <tr>
      <th>Position</th>
      <th>League</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @league_roster_slots.each do |league_roster_slot| %>
      <tr>
        <td><%= league_roster_slot.position_id %></td>
        <td><%= league_roster_slot.league_id %></td>
        <td><%= link_to 'Show', league_roster_slot %></td>
        <td><%= link_to 'Edit', edit_league_roster_slot_path(league_roster_slot) %></td>
        <td><%= link_to 'Destroy', league_roster_slot, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New League roster slot', new_league_roster_slot_path %>
